    <div>
      {{product.metafields.sf_product_tabs.tab_5_content}}
    </div>
  {% endif %}
  <h3 id="pp_accor-6">Prop 65</h3>
  <div id="pp_accor_content-6">
    <div class="cstMob_65_warning">
      <div class="cstMob_65_icon">
        <img src="{{'warning-sign.svg' | asset_url}}" />
      </div>
      <div>
        <p>WARNING: This product can expose you to chemicals including nickel and Di-siodecyl phthalate, which are known to the State of California to cause cancer or birth defects or other reproductive harm.</p>
        <label>For more information visit <a href="https://www.p65warnings.ca.gov/" target="_blank">www.p65warnings.ca.gov</a>.</label>
      </div>
    </div>  
  </div>
  {% comment %} <h3>Overview</h3>
  <div>
 {{product.metafields.global.overview}}
    <div class="Btn-more"><button  id="myBtn">Show more</button></div>
  </div>
  <h3>Feature</h3>
  <div>
   {{product.metafields.global.feature}}
  </div>
  <h3>Specification</h3>
  <div>
    {{product.metafields.global.specification}}
  </div>
  <h3>Resources</h3>
  <div>
   {{product.metafields.global.resources}}
  </div> {% endcomment %}
</div>