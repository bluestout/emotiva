<div id="Proaccordion">
  {% if product.metafields.sf_product_tabs.tab_1_title != blank %}
    <h3>{{product.metafields.sf_product_tabs.tab_1_title}}</h3> 
    <div>
      {{product.metafields.sf_product_tabs.tab_1_content}} 
      {% comment %} <div class="Btn-more"><button  id="myBtn">Show more</button></div> {% endcomment %}
    </div>
  {% endif %}
  {% if product.metafields.sf_product_tabs.tab_2_title != blank %}
    <h3>{{product.metafields.sf_product_tabs.tab_2_title}}</h3>
    <div>
    {{product.metafields.sf_product_tabs.tab_2_content}}
    </div>
  {% endif %}
  {% if product.metafields.sf_product_tabs.tab_3_title != blank %}
    <h3>{{product.metafields.sf_product_tabs.tab_3_title}}</h3>
    <div>
      {{product.metafields.sf_product_tabs.tab_3_content}}
    </div>
  {% endif %}
  {% if product.metafields.sf_product_tabs.tab_4_title != blank %}
    <h3>{{product.metafields.sf_product_tabs.tab_4_title}}</h3>
    <div>
      {{product.metafields.sf_product_tabs.tab_4_content}}
    </div>
  {% endif %}
  {% if product.metafields.sf_product_tabs.tab_5_title != blank %}
