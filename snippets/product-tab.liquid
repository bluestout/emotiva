<div id="CustomProtabs" style="display:inline-block;width:100%;">
  <ul>
    {% if product.metafields.sf_product_tabs.tab_1_title != blank %}
      <li><a href="#tabs-1">{{product.metafields.sf_product_tabs.tab_1_title}}</a></li>
    {% endif %}
    {% if product.metafields.sf_product_tabs.tab_2_title != blank %}
      <li><a href="#tabs-2">{{product.metafields.sf_product_tabs.tab_2_title}}</a></li>
    {% endif %}
    {% if product.metafields.sf_product_tabs.tab_3_title != blank %}
      <li><a href="#tabs-3">{{product.metafields.sf_product_tabs.tab_3_title}}</a></li>
    {% endif %}
    <li><a href="#tabs-4">Prop 65</a></li>
  </ul>
  {% if product.metafields.sf_product_tabs.tab_1_content != blank %}
    <div id="tabs-1">
    {{product.metafields.sf_product_tabs.tab_1_content}}
    </div>
  {% endif %}
  {% if product.metafields.sf_product_tabs.tab_2_content != blank %}
    <div id="tabs-2">
      {{product.metafields.sf_product_tabs.tab_2_content}}
    </div>
  {% endif %}
  {% if product.metafields.sf_product_tabs.tab_3_content != blank %}
    <div id="tabs-3">
    {{product.metafields.sf_product_tabs.tab_3_content}}
    </div>
  {% endif %}
  {% if product.metafields.sf_product_tabs.tab_4_content != blank %}
    <div id="tabs-4">
    {{product.metafields.sf_product_tabs.tab_4_content}}
    </div>
  {% endif %}
  {% if product.metafields.sf_product_tabs.tab_5_content != blank %}
    <div id="tabs-5">
    {{product.metafields.sf_product_tabs.tab_5_content}}
    </div>
  {% endif %}
  <div id="tabs-6">
    <p>WARNING: This product can expose you to chemicals including nickel and Di-siodecyl phthalate, which are known to the State of California to cause cancer or birth defects or other reproductive harm.</p>
    <p>For more information visit <a href="https://www.p65warnings.ca.gov/" target="_blank">www.p65warnings.ca.gov</a>.</p>
  </div>
  {% comment %} <ul> 
    <li><a href="#tabs-1">Overview</a></li>
    <li><a href="#tabs-2">Feature</a></li>
    <li><a href="#tabs-3">Specification</a></li>
    <li><a href="#tabs-4">Resources</a></li>
  </ul>
  <div id="tabs-1">
   {{product.metafields.global.overview}}
  </div>
  <div id="tabs-2">
    {{product.metafields.global.feature}}
  </div>
  <div id="tabs-3">
  {{product.metafields.global.specification}}
  </div>
  <div id="tabs-4">
  {{product.metafields.global.resources}}
  </div> {% endcomment %}
</div>