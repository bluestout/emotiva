<style>
.ca-notification  .popup{
    display:none;
}
.ca-notification .popup {
    position: absolute;
    background: #f3f3f3;
    color: #000;
    padding: 10px;
    max-width: 500px;
    /* display: none; */
    top: 70px;
    left: 0px;
    z-index: 99;
    border-radius: 5px;
    border: 1px solid #d3d3d3;
    font-size: 12px;
}
 td.product__description:hover .popup {
    display: block;
}
.notification-text{
   color:#f3f3f3;
}
.ca-notification .popup:before {
    content: '';
    border-left: 10px solid transparent;
    border-right: 10px solid  transparent;
    border-top: 10px solid transparent;
    border-bottom: 10px solid #f3f3f3;
    position: absolute;
    top: -20px;
    width: 0;
    height: 0;
    left: 50%;
    transform: translateX(-50%);
}
td.product__description {
  position: relative;
  overflow: visible;
}
.order-summary__section{
  overflow:visible !important;
}
      
.no-js .order-summary__section__content,.order-summary__section__content {
  overflow: visible!important;
  z-index: 9;
}
      
@media screen and (min-width: 1200px) {
   .ca-notification	.popup {
    width: 400px; 
    left: -80px;
}
}
@media screen and (min-width: 1280px) {
   .ca-notification.popup {
    width: 500px; 
    left: -110px;
   }
}
  /*FS style*/

@media (min-width: 749px)
{
  .order-summary__section__content
  {
    max-height: 300px;
    overflow-y: scroll !important;
  }
  .order-summary__section--product-list .product-table {
    max-width: 400px;
  }
}
/***************international popup************************/
.inter_ship_popup_hide
{
  display: none !important;
}

.inter_ship_popup_bg
{
  position: fixed;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
  z-index: 9999;
  background-color: rgba(0,0,0,0.5);
}
.inter_ship_popup
{
  position: fixed;
  left: 0;
  right: 0;
  top: 160px;
  background-color: #fff;
  max-width: 350px;
  margin: 0 auto;
  z-index: 10000;
}

.inter_ship_popup_ini
{
  border: 1px solid #F0F1F2;
  box-shadow: 0 10px 20px 0 rgb(0 0 0 / 13%);
  padding: 24px;
}
.close-popup-img {
  cursor: pointer;
  position: relative;
}
.close-popup-img:after{
  clear: both;
  display: table;
  content:"";
}
.close-popup-icon {
    width: 16px;
    height: 16px;
    position: relative;
    display: block;
    float: right;
}
.close-popup-icon:before {
    width: 100%;
    left: 0;
    top: 50%;
    transform: rotate(45deg);
    content: "";
    background: #667175;
    height: 1px;
    position: absolute;
}
.close-popup-icon:after {
    width: 100%;
    left: 0;
    top: 50%;
    transform: rotate(-45deg);
    content: "";
    background: #667175;
    height: 1px;
    position: absolute;
}
.inter_ship_title{
  margin-bottom: 16px;
}
.inter_ship_cont{
  margin-bottom: 16px;
}
.inter_ship_accep_row
{
  text-align: center;
}
.inter_ship_accep_chkbox:focus
{
  outline: none;
}
.inter_ship_accep_btn
{
  display: block;
  padding: 5px 20px;
  margin: 20px auto 0;
}
.inter_ship_notify_hide
{
  display: none !important;
}
.inter_ship_notify p
{
 font-size: 13px;
 margin-bottom: 12px;
}
.checkout_international_notify_hide
{
  {% comment %} display: none; {% endcomment %}
}
.checkout_international_notify
{
  text-align: center;
  margin-top: 50px;
}
</style> 
    

    
