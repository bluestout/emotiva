<script type="text/javascript">
window.onload = function(e){ 
   function myFunction(){
       var selectedVal = document.getElementById("checkout_shipping_address_province").value = this.value;
       if(selectedVal == 'California'){
         createCookie("shipping_address_province",selectedVal);
         //caNotification();
       }
       else{
         createCookie("shipping_address_province","",'Thu, 01 Jan 1970 00:00:01 GMT');
       }
    } 
      var element =  document.getElementById('checkout_shipping_address_province');
      if (typeof(element) != 'undefined' && element != null)
        {
            document.getElementById("checkout_shipping_address_province").addEventListener("change", myFunction);
        }

      var temp_cok= readCookie('shipping_address_province');
      if(temp_cok == 'California' && temp_cok != undefined) {
            caNotification();
      }
}
  
var caNotification = function(){
  var tablearray = document.querySelectorAll(".order-summary__section__content .product-table tbody tr"); 
  var i = 0 , len = tablearray.length , exclude = ['485900779563','485899042859'] ; 
  for(; i < len ; i++ ){
    //console.log(i);
    var tr = tablearray[ i ] , id = tr.getAttribute("data-product-id"); 
    if(exclude.indexOf(id)>-1) continue ;  	
    var td = tr.getElementsByClassName("product__description") ; 
    if(!td.length) continue; td[0].innerHTML+="<div class='ca-notification'><span class='notification-text'>CA Prop 65 Consumer Notice</span><div class='popup'>⚠️ WARNING: This product can expose you to chemicals including nickel and Di-isodecyl phthalate, which are known to the State of California to cause cancer or birth defects or other reproductive harm.<br />For more information visit www.p65warnings.ca.gov.</div></div>";       
  }
}

  var createCookie = function(name,value,days){
      	var expires = "";
		if (days) {
			var date = new Date();
			date.setTime(date.getTime() + (days*24*60*60*1000));
			expires = "; expires=" + date.toUTCString();
		}
		document.cookie = name + "=" + value + expires + "; path=/";
  }
  var readCookie=function(name){
      	var nameEQ = name + "=";
		var ca = document.cookie.split(';');
		for(var i=0;i < ca.length;i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1,c.length);
			if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
		}
		return null;
  }  
  
  $(document).ready(function(){ 
    function checkInternation()
    {
      var country = "{{ checkout.shipping_address.country }}";
      if( country !== "United States")
      {      
        var current_tab = $('.breadcrumb__item--current .breadcrumb__text').text();
        if(current_tab == 'Payment')
        {
          $('#inter_ship_popup').removeClass("inter_ship_popup_hide");
        }
      }
      
      $('body').on('click', '.close-popup-img', function(e){
        e.preventDefault();
        $('#inter_ship_popup').addClass("inter_ship_popup_hide");
      })  

      $(".inter_ship_accep_chkbox").change(function(){
        if ($('input.inter_ship_accep_chkbox').is(':checked')) {
          $(".inter_ship_accep_btn").removeAttr("disabled");
        }    
        else{
          $(".inter_ship_accep_btn").attr("disabled", true);
        }
      })
      $('body').on('click', '.inter_ship_accep_btn', function(e){
        e.preventDefault();
        $('#inter_ship_popup').addClass("inter_ship_popup_hide");
        $('#inter_ship_notify').removeClass("inter_ship_notify_hide");
        

      })  
      
      
    }
     setTimeout(checkInternation, 1000)

  });
  
</script>
 
