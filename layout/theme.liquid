<body class="template-{{ request.page_type | handle }} {% if template == "page.landing"%} CustomLanding{%endif%} {% if template == "page.resources3"%} no-side-bar-mob-title{%endif%}">
{% include 'slidecart' %}
  {%include "video-popup" %}
  <a class="in-page-link visually-hidden skip-link" href="#MainContent">{{ 'general.accessibility.skip_to_content' | t }}</a>

  {%- if settings.enable_ajax -%}
    {% include 'cart-popup' %}
  {%- endif -%}

  {% section 'header' %}

  <div class="page-container drawer-page-content" id="PageContainer">

    <main class="main-content js-focus-hidden" id="MainContent" role="main" tabindex="-1">
      {% if template contains "article" %}
  <div class="em-articl-sub-nav-sec">
  <div class="em-articl-sub-nav-list">
    <ul>
         {% for link in linklists['blogs'].links %}
      <li><a href="{{ link.url }}">{{ link.title }}</a></li>
      {% endfor %}
    </ul>
  </div>
</div>
  {%elsif template contains "blog.custom" %}
   <div class="em-articl-sub-nav-sec">
  <div class="em-articl-sub-nav-list">
    <ul class="BlogCustomize">
     {% for link in linklists['blogs'].links %}
      <li><a href="{{ link.url }}">{{ link.title }}</a></li>
      {% endfor %}
          
    </ul>
  </div>
</div>
  {%endif%}
      {%if page.handle contains "support-2" or page.handle contains "customer-center0" or page.handle contains "customer-center1" or page.handle contains "customer-center2" or page.handle contains "resources3" %}
       <div class="mobile-pages-breadcrumb">
 <div class="page-width">
{%include "breadcrumbs" %}
         </div>
</div>
      {%endif%}
      {{ content_for_layout }}
  {%if page.handle contains "contact-1" or template == "page.landing" or page.handle contains "customer-center0" or page.handle contains "customer-center1" or page.handle contains "customer-center2" or page.handle contains "resources3" or page.handle contains "support-1" or page.handle contains "support-2" or page.handle contains "resources-10" or page.handle contains "resources-1" or page.handle contains "resources-2" or page.handle contains "resources-4"%}
 
      {%else%}
        {% section "Newsletter-custom" %}
      {%endif%}
      {% unless template == "page.landing"%}
      {% section "light-bg-icon" %}
      {%endunless%}
    </main>
    

    

    
    
    {% unless template == "page.landing"%}

    {% section 'footer' %}
{%endunless%}
    <div id="slideshow-info" class="visually-hidden" aria-hidden="true">
      {{- 'sections.slideshow.navigation_instructions' | t -}}
    </div>

  </div>

  <ul hidden>
    <li id="a11y-refresh-page-message">{{ 'general.accessibility.refresh_page' | t }}</li>
    <li id="a11y-selection-message">{{ 'general.accessibility.selection_help' | t }}</li>
  </ul>
  <script async type="text/javascript" data-api-key="pubkey-DhCVJf5UrKPig7T6z22MAY54I577yP" id="stamped-script-widget" src="https://cdn2.stamped.io/files/widget.min.js"></script>
