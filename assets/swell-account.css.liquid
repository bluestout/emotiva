.swell-account-text {
    font-family: Roboto;
    font-size: 42px;
    color: black;
    text-align: center;
    font-weight: 500;
    letter-spacing: 1px;
}
.swell-account .swell-header-container {
	background-image:unset;
	height: unset;
}
.swell-account .swell-header-text  {
	display: none;
}
.swell-account .swell-header-login-details h2 {
	color: black;
	text-align: center;
}
.swell-account .swell-header-login-details{
	    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    align-items: center;
    border-top:unset;
    width: 100%;
}
.swell-account .swell-header-login-details h2 {
	width: 100%;
}
.swell-account .swell-header-login-container {
	    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    align-items: center;
}
.swell-account .swell-header-btn {
	width: 100%;
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    align-items: center;
}
.swell-account .swell-header-btn button:nth-of-type(2){
	border: .5px solid #003e71;
}
.swell-account .swell-header-details {
    margin-top: 0;
    width: 100%;
}
.swell-account  .swell-vip-tiers-container {
	width: 80%;
}
.swell-account .swell-referral-banner {
	display: none;
}
.swell-account .swell-header-login-container {
    padding-left: 0;
}
@media only screen and (max-width: 910px) {
	.swell-account .swell-header-login-container {
		background-image:unset;
	}
}
@media only screen and (max-width: 910px)  {
	 .swell-account-text {
	 	    font-size: 30px;
	 	    letter-spacing:unset;

	}
}
