.CustomLanding #shopify-section-header{
	position: fixed;
  left: 0;
  top: 0;
  right: 0;
  z-index: 11;
}
.CustomLanding > .page-container{
	transform: none;
  -moz-transform: none;
  -webkit-transform: none;
}
.CustomLanding .page-container main.main-content{
	background-image: none !important;
  background-color: #242424;
  padding-top: 0px;
}
.CustomLanding .page__container--secondary .main .expert-help{
	display: none !important;
}
.em-landing-pg-main:after{
  clear: both;
  display: table;
  content:"";
}
.em-landing-pg-main .em-landing-pg-img{
  position: fixed;
  top: 79px;
  bottom: 0;
  right: 0;
  width: 58.34%;
  background-size: cover;
  background-position: 50% 100%;
}
.em-landing-pg-img-title{
  font-size: 32px;
  line-height: 34px;
  font-weight: 700;
  color: #fff;
  text-transform: none;
  letter-spacing: .2px;
  padding: 34px 23px 0px;
  margin: 0;
}
.em-landing-pg-main .em-landing-pg-form{
  width: 41.66%;
  float: left;
  padding: 76px 0px 120px;  
}
.em-landing-pg-form-inn{
	max-width: 485px;
  padding-left: 15px;
  padding-right: 15px;
  margin: auto;
}
.em-landing-pg-form-title{ 
  color: #FFFFFF;
  font-size: 44px;
  font-weight: bold;
  letter-spacing: 0.28px;
  line-height: 43px;
  margin: 100px 0 27px 0;
}
.em-landing-pg-form-desc{
	  color: #FFFFFF;
  font-size: 18px;
  letter-spacing: 0.11px;
  line-height: 27px;
  margin: 0 0 25px 0;
}
.em-landing-pg-form-bot ul{
	margin: 0;
  padding: 0;
  list-style: none;
}
.em-landing-pg-form-bot ul:after{
  clear: both;
  display: table;
  content:"";
}
.em-landing-pg-form-bot ul li{
  float: left;
  width: 50%;
  margin-bottom: 7px;
}
.em-landing-pg-form-bot ul li:nth-child(1){
	padding-right: 3px;
}
.em-landing-pg-form-bot ul li:nth-child(2){
	padding-left: 3px;
}
.em-landing-pg-form-bot ul li:last-child{
	width: 100%;
}
.em-landing-pg-form-btn{
  border: 1px solid rgba(255,255,255,0.1);
  border-radius: 1px;
  display: block;
  padding: 16.5px;
  position: relative;
  color: rgba(255,255,255,0.6);
  font-size: 16px;
  letter-spacing: 0.1px;
  line-height: 19px;
  min-height: 69px;
}
.em-landing-pg-form-btn-vid img{
	left: 21.5px;
  top: 17.5px;
  position: absolute;
  height: 34px;
}
.em-landing-pg-form-btn-user img{
	left: 19.5px;
  top: 17.5px;
  position: absolute;
  height: 37px;
}
.em-landing-pg-form-btn-manual img{
	left: 22.5px;
  top: 18.5px;
  position: absolute;
  height: 18px;
}
.em-landing-pg-form-btn.em-landing-pg-form-btn-vid{
	padding-left: 72.5px;
}
.em-landing-pg-form-btn.em-landing-pg-form-btn-user{
	padding-left: 60.5px;
}
.em-landing-pg-form-btn.em-landing-pg-form-btn-manual{
	padding-left: 54.5px;
}
.em-landing-pg-copy-txt{
  position: fixed;
  bottom: 20px;
  right: 23px;
}
.em-landing-pg-copy-txt p{
	  opacity: 0.5;
  color: #242424;
  font-size: 11px;
  letter-spacing: 0.07px;
  line-height: 13px;
  margin: 0;
}
/*form layour starts*/
.em-landing-pg-form-sec{
	margin-bottom: 30px;
}
.em-landing-pg-form-sec .page__content-wrapper{
	padding: 0;
}
.em-landing-pg-form-sec .page__content-wrapper .container{
	padding: 0;
}
.em-landing-pg-form-sec .form__control{
	margin-bottom: 7px;
}
.em-landing-pg-form-sec .form__control-row{
	margin-bottom: 0px;
}
.em-landing-pg-form-sec .form__input{
	border: none;
  font-size: 16px;
  letter-spacing: 0.1px;
  line-height: 28px;
  color: #242424;
    padding: 9px 15px;
  display: block;
    width: 100%;
  outline: none;
  border-radius: 0px;
}
.em-landing-pg-form-sec select{
	height: 46px;
    outline: none;
    width: 100%;
    color: #242424;
    padding: 0 15px;
    font-size: 16px;
    border: none !important;
    letter-spacing: 0.1px;
  border-radius: 0px;
}
.em-landing-mac-fld-tooltip-block{
	position: relative;
}
.em-landing-mac-fld-tooltip-block .em-landing-mac-fld-tooltip{
	position: absolute;
    top: 9px;
    right: 11px;
}
.em-landing-mac-fld-tt-hover{
	position: absolute;
  bottom: 100%;
  padding-bottom: 10px;
  width: 256px;
  left: -20px;
  display: none;
}
.em-landing-mac-fld-tooltip:hover .em-landing-mac-fld-tt-hover{
	display: block;
}
.em-landing-mac-fld-tt-hover p{
  background-color: #FFFFFF;
  box-shadow: 0 6px 20px 0 rgba(0,0,0,0.19);
  padding: 20px;
  color: #000000;
  font-size: 13px;
  letter-spacing: 0.08px;
  line-height: 19px;
}
.em-landing-pg-form-sec .em-landing-mac-fld-tooltip-block .form__input{
	padding-right: 35px;
}
.em-landing-pg-form-sec input[type='submit']{
  font-size: 15px;
  font-weight: bold;
  letter-spacing: 2.09px;
  line-height: 18px;
  text-align: center;
  color: #fff;
  border: none !important;
  outline: none !important;
  width: 100%;
  padding: 14px;
  background-color: #00457c;
  transition: all 0.25s ease-in-out;
  outline: none;
}
.em-landing-pg-form-sec input[type='submit']:hover{
	background-color: #001a2f;
}
.em-landing-form-thank-open .em-landing-pg-form-inn{
  min-height: 790px;
}
.em-landing-form-thank-open .em-landing-pg-form-sec{
  margin-top: 60px;
}
.em-landing-form-thank-open .em-landing-pg-form-title{
	display: none;
}
.em-landing-form-thank-open .em-landing-pg-form-desc{
	display: none;
}
.em-landing-form-thank-open .em-landing-pg-form-bot{
	display: none;
}
.alert--success.em-landing-success-info{
	padding: 100px 0 0;
  background: none;
}
.em-landing-success-alert__title{ 
  color: #FFFFFF;
  font-size: 44px;
  font-weight: bold;
  letter-spacing: 0.28px;
  line-height: 43px;
  margin: 27px 0;
}
.em-landing-success-alert__desc{
	  color: #FFFFFF;
  font-size: 18px;
  letter-spacing: 0.11px;
  line-height: 27px;
  margin: 0 0 25px 0;
  font-weight: 400;
}
.em-landing-success-alert__btn{
	padding-top: 40px;
}
.em-landing-success-alert__btn a{
    font-size: 15px;
    font-weight: bold;
    letter-spacing: 2.09px;
    line-height: 18px;
    text-align: center;
    color: #fff;
    border: none !important;
    outline: none !important;
    width: 100%;
    padding: 14px;
}
.em-landing-success-alert__btn a{
	background-color: #00457C;
  display: block;
  text-align: center;
    color: #FFFFFF !important;
  font-size: 15px;
  font-weight: bold;
  letter-spacing: 2.09px;
  line-height: 18px;
  padding: 14px;
  text-transform: uppercase;
  
}
/*form layout ends*/

/*-----------for dirac From-----------*/

.globo-form-app .globo-label
{
    display:none !important;
}
.globo-form-app .globo-form-control .text-smaller
{
  display:none;
}
.g-container p
{
  display:none;
}

.globo-form-app .globo-form-control
{
  margin-bottom: 5px !important; 
}
.globo-form-app .message+.footer
{
  margin-top: 5px !important; 
}

.powerfull-dirac-form-hide
{
  display: none;
}
.powerfull-dirac-form-success-show
{
  display: block !important;
}
.powerfull-dirac-form-success-hide
{
  display: none;
}


/*responsive layout*/
@media(min-width:750px){
  .em-landing-pg-img-title{
  	display: none;
  }
  .em-landing-pg-img img{
  	display: none;
  }
  .form__control-row > .form__control {
    display: inline-block; 
  }
  .em-landing-pg-form-sec .form__control-row > .form__control{
	width: calc(50% - 6px);
    vertical-align: top;
  }
  .em-landing-pg-form-sec .form__control-row > .form__control:last-child {
      margin-left: 8px;
  }
}
@media(max-width:749px){
  .em-landing-mac-fld-tt-hover{
  	left: auto;
    right: -11px;
  }
  .em-landing-pg-main .em-landing-pg-img{
  	position: relative;
    top: 0!important;
    width: 100%;
  }
  .em-landing-pg-main .em-landing-pg-img .em-landing-pg-img-title{
  	position: absolute;
    left: 0;
    top: 0;
  }
  .em-landing-pg-main .em-landing-pg-form{
  	width: 100%;
    float: none;
    padding: 37px 0;
  }
  .em-landing-pg-main .em-landing-pg-form .em-landing-pg-form-title{
  	display: none;
  }
  .em-landing-success-alert__desc{
  	font-size: 16px;
    line-height: 23px;
    letter-spacing: 0.1px;
  }
  .em-landing-success-alert__title{
  	font-size: 32px;
    line-height: 34px;
    letter-spacing: 0.2px;
  }
  .em-landing-pg-form-desc{
  	font-size: 16px;
    line-height: 23px;
    letter-spacing: 0.1px;
  }
  .em-landing-pg-form-inn{
  	padding-left: 25px;
    padding-right: 25px;
  }
  .em-landing-pg-form-bot ul li{
    float: left;
    width: 100%;
    margin-bottom: 7px;
  }
  .em-landing-pg-form-bot ul li:nth-child(1){
      padding-right: 0px;
  }
  .em-landing-pg-form-bot ul li:nth-child(2){
      padding-left: 0px;
  }
  .em-landing-pg-form-bot ul li:last-child{
      width: 100%;
  }
  .em-landing-pg-form-btn{
    border: 1px solid rgba(255,255,255,0.1);
    border-radius: 1px;
    display: block;
    padding: 15.5px;
    position: relative;
    color: rgba(255,255,255,0.6);
    font-family: Roboto;
    font-size: 15px;
    letter-spacing: 0.1px;
    line-height: 18px;
    min-height: 53px;
  }
  .em-landing-pg-form-btn br{
  	display: none !important;
  }
  .em-landing-pg-form-btn-vid img{
      left: 21.5px;
    top: 15.5px;
    position: absolute;
    height: 22px;
  }
  .em-landing-pg-form-btn-user img{
      left: 23.5px;
    top: 14.5px;
    position: absolute;
    height: 25px;
  }
  .em-landing-pg-form-btn-manual img{
      left: 22.5px;
    top: 14.5px;
    position: absolute;
    height: 18px;
  }
  .em-landing-pg-form-btn.em-landing-pg-form-btn-vid{
      padding-left: 60.5px;
  }
  .em-landing-pg-form-btn.em-landing-pg-form-btn-user{
      padding-left: 60.5px;
  }
  .em-landing-pg-form-btn.em-landing-pg-form-btn-manual{
      padding-left: 54.5px;
  }
  .em-landing-pg-copy-txt{
    position: static;
    bottom: 20px;
    right: 23px;
  }
  .em-landing-pg-copy-txt p{
  	color: #fff;
    opacity: .2;
    font-size: 11px;
    line-height: 13px;
    padding: 28px 25px 24px;
  }
}
a#dwn-btn {
    cursor: pointer;
}