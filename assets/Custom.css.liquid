}
.em-hm-fea-block-hover{
	position: absolute;
  top: -20px;
    left: -20px;
    text-align: center;
    right: -20px;
  opacity: 0;
  visibility: hidden;
  box-shadow: 0px 12px 23px rgba(102,113,117,0.18);
  -moz-box-shadow: 0px 12px 23px rgba(102,113,117,0.18);
  -webkit-box-shadow: 0px 12px 23px rgba(102,113,117,0.18);
  background-color: #fff;
  padding: 20px;
  transition: all 400ms cubic-bezier(0.230, 1.000, 0.320, 1.000);
  -moz-transition: all 400ms cubic-bezier(0.230, 1.000, 0.320, 1.000);
  -webkit-transition: all 400ms cubic-bezier(0.230, 1.000, 0.320, 1.000);
  z-index: 2;
}
.em-hm-fea-block:hover .em-hm-fea-block-hover{
	opacity: 1;
  visibility: visible;
}
.em-hm-fea-block-hover p{
	margin: 20px 0 30px 0;
  color: #000;
  font-size: 14px;
  line-height: 23px;
