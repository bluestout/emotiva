@font-face {
    font-family: 'neutrif_pro';
    src: url('neutrifpro-bold-webfont.woff2') format('woff2'),
         url('neutrifpro-bold-webfont.woff') format('woff');
    font-weight: 700; 
    font-style: normal;
}
@font-face {
    font-family: 'neutrif_pro';
    src: url('neutrifpro-regular-webfont.woff2') format('woff2'),
         url('neutrifpro-regular-webfont.woff') format('woff');
    font-weight: 400; 
    font-style: normal;
} 

 
body{
	font-family: 'neutrif_pro' !important;  
}
h1, .h1, h2, .h2, h3, .h3, h4, .h4, h5, .h5, h6, .h6{
	font-family: 'neutrif_pro' !important;
} 

/*header starts*/
#shopify-section-header{
	position: fixed;
  left: 0;
  top: 0;
  right: 0;
  z-index: 11;
}
header.site-header.logo--center{
	padding-top: 0px;
} 
header.site-header{
	padding-left: 0px;
  padding-right: 0px;
}
.em-top-head-sec .em-ext-top-bar{
	background-color: #000;
  padding: 7px;
}
.em-top-head-sec .em-ext-top-bar p{
	font-size: 13px;
  line-height: 16px;
  color: #fff;
  text-align: center;
}
.em-top-head-sec .em-head-top-bar{
	padding: 20px 55px;
  border-bottom: 1px solid rgba(102,113,117,0.1);
}
.em-head-top-bar-row:after{
	clear: both;
  display: table;
  content: ""; 
}
.em-head-top-bar-row .em-head-top-bar-left{
	width: 32%;
  float: left;
  padding-top: 7px;
}
.em-head-top-bar-row .em-head-top-bar-middle{
	width: 36%;
  float: left;
}
.em-head-top-bar-row .em-head-top-bar-right{
	width: 32%;
  float: left;
  text-align: right;
  padding-top: 7px;
}
.em-head-top-bar-logo{
	max-width: 190px;
  	display: inline-block;
    vertical-align: bottom;
  margin-right: 15px;
}
.em-head-top-bar-logo img{
	display: block;
}
.em-head-top-bar-left > p{
	font-size: 13px;
    line-height: 20px;
    color: #667175;
  display: inline-block;
    vertical-align: bottom;
  margin: 0;
}
.em-head-top-bar-rt-list{
	margin: 0;
  padding: 0;
  list-style: none;
}
.em-head-top-bar-rt-list li{
	margin-left: 40px;
  font-size: 16px;
    line-height: 20px;
    color: #000;
  display: inline-block;
}
.em-head-top-bar-rt-list li:first-child{
	margin-left: 0px;
}
.em-head-top-bar-rt-list li a{
	font-size: 16px;
    line-height: 20px; 
    color: #000;
}
.em-head-top-bar-middle .search-bar__input{
	min-height: 40px;
  border-bottom: 1px solid rgba(102,113,117,0.25);
  border-radius: 2px;
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
  font-size: 14px;
  line-height: 18px;
  color: rgba(102,113,117.0.3);
  padding-right: 18px;
  padding-left: 45px;
}
.em-head-top-bar-middle .search-bar__form .search-bar__submit{
	right: auto;
  left: 0;
  outline: none;
  padding: 0 14px 0 18px;
}
.em-head-top-bar-middle .search-bar__form .search-bar__submit svg{
	opacity: .2;
  width: 14px;
}

/*header ends*/
/*nav starts*/
.em-hm-about-main{
