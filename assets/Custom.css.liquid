@keyframes inf-logo-slide {
  to {
    transform:translate3D(-100%,0,0);
  }
}
.em-logo-inf-slider-main{
  width: 100%;
  display: flex;
  flex-flow: row nowrap;
  overflow: hidden;
  z-index: 1;
  position: relative;
}
.em-logo-inf-slider-main::before {
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 15%;
  height: 92px;
  background-image: linear-gradient(90deg,#fdfdff 72%,rgba(253,253,255,.8) 84%,rgba(253,253,255,0) 94%);
  z-index: 2;
}
.em-logo-inf-slider-main::after{
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  right: 0;
  width: 15%;
  height: 92px;
  background-image: linear-gradient(90deg,#fdfdff 72%,rgba(253,253,255,.8) 90%,rgba(253,253,255,0));
  transform: rotate(-180deg);
  z-index: 2;
}
.em-logo-inf-slider-inn{
  display: flex;
