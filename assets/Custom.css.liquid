/*sticky header starts*/
.em-top-head-sec-sticky{
	padding: 0 55px;
  position: fixed;
  left: 0;
  right: 0;
  top: -100%;
  opacity: 0;
  background-color: #fff;
  z-index: 111;
  transition: all 2s ease-out;
  -moz-transition: all 2s ease-out;
  -webkit-transition: all 2s ease-out;
  border-bottom: 1px solid rgba(102,113,117,0.1);
}
.em-top-head-sec-sticky.em-top-head-sec-sticky-open{
  opacity: 1;
	top: 0px;
  transition: all 2s ease-out;
  -moz-transition: all 2s ease-out;
  -webkit-transition: all 2s ease-out;
}
.em-top-head-sec-sticky-inn{	
  padding-left: 150px;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
  -ms-flex-pack: justify;
  justify-content: space-between;
}
.em-top-head-sec-sticky .em-articl-sub-nav-sec{
	padding: 0 55px;
    margin: 0px -55px;
    border-bottom: 0px solid #e2e4e6;
  border-top: 1px solid rgba(102,113,117,0.1);
}
.em-top-head-sec-sticky-inn .em-top-head-sec-sticky-logo{
	position: absolute;
  left: 55px;
  top: 24px;
  width: 130px;
}
.em-top-head-sec-sticky-inn .em-nav-top-bar-lt-list > li{
	margin-left: 20px;
}
.em-top-head-sec-sticky-inn .em-nav-top-bar-lt-list > li > a{
